<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>login.html</welcome-file>
    </welcome-file-list>
    <context-param>
        <param-name>view-prefix</param-name>
        <param-value>/</param-value>
    </context-param>
    <context-param>
        <param-name>view-suffix</param-name>
        <param-value>.html</param-value>
    </context-param>


    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>com.user.servlet.loginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>repair</servlet-name>
        <servlet-class>com.user.servlet.addServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>repair</servlet-name>
        <url-pattern>/repair</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>file</servlet-name>
        <servlet-class>com.user.servlet.repairFormServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>file</servlet-name>
        <url-pattern>/repairForm</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>cardServlet</servlet-name>
        <servlet-class>com.user.servlet.findCardByIdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>cardServlet</servlet-name>
        <url-pattern>/card</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>findCardByIdServlet</servlet-name>
        <servlet-class>com.user.servlet.findCardByIdServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>findCardByIdServlet</servlet-name>
        <url-pattern>/search</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>updateMoneyServlet</servlet-name>
        <servlet-class>com.user.servlet.updateMoneyServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateMoneyServlet</servlet-name>
        <url-pattern>/chongzhi</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>updateWaterServlet</servlet-name>
        <servlet-class>com.user.servlet.updateWaterServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateWaterServlet</servlet-name>
        <url-pattern>/shuidian</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>updateElectricServlet</servlet-name>
        <servlet-class>com.user.servlet.updateElectricServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>updateElectricServlet</servlet-name>
        <url-pattern>/wangluo</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LeaveRecordServlet</servlet-name>
        <servlet-class>com.user.servlet.LeaveRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LeaveRecordServlet</servlet-name>
        <url-pattern>/leave_record</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>LeaveServlet</servlet-name>
        <servlet-class>com.user.servlet.LeaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>LeaveServlet</servlet-name>
        <url-pattern>/leave</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>feedServlet</servlet-name>
        <servlet-class>com.user.servlet.FeedServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>feedServlet</servlet-name>
        <url-pattern>/feed</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>feed_recordServlet</servlet-name>
        <servlet-class>com.user.servlet.FeedRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>feed_recordServlet</servlet-name>
        <url-pattern>/feedback_record</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>tLeaveRecordServlet</servlet-name>
        <servlet-class>com.user.servlet.tLeaveRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tLeaveRecordServlet</servlet-name>
        <url-pattern>/all_leave</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>OperateLeaveServlet</servlet-name>
        <servlet-class>com.user.servlet.OperateLeaveServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OperateLeaveServlet</servlet-name>
        <url-pattern>/operate_leave</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>CourseServlet</servlet-name>
        <servlet-class>com.user.servlet.CourseServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CourseServlet</servlet-name>
        <url-pattern>/course</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>tFeedRecordServlet</servlet-name>
        <servlet-class>com.user.servlet.tFeedRecordServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>tFeedRecordServlet</servlet-name>
        <url-pattern>/tfeed_record</url-pattern>
    </servlet-mapping>



</web-app>